	private int sortStringId;
	
	public DownloaderStandardSortMsg(Context curContext, Class<?> newActivity, int sortStringId) {
		super(curContext, newActivity);
		this.sortStringId = sortStringId;
	}

	@Override  
    protected void doIfSuccessed()  
    {  
		super.doIfSuccessed();
		
    }	
	
	private void displayToast(){
		curContext.make
	}
}
